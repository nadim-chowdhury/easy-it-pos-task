# Database
DATABASE_URL="postgresql://username:password@localhost:5432/pos_db"
DB_POOL_MIN=2
DB_POOL_MAX=10
DB_POOL_IDLE=10000
DB_POOL_ACQUIRE=60000
DB_POOL_EVICT=1000
DB_CONNECT_TIMEOUT=60000
DB_QUERY_TIMEOUT=30000

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
