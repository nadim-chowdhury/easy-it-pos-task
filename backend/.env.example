RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info

# JWT
JWT_SECRET=your-super-secret-jwt-key
JWT_EXPIRES_IN=7d

# Server
PORT=3001
NODE_ENV=development

FRONTEND_URL=''